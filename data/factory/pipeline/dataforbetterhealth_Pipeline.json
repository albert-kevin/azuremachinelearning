{
	"name": "dataforbetterhealth_Pipeline",
	"properties": {
		"description": "exercise for collecting data\nas was done on a hackathon at Digityser 2019\nsee if we can collect all resources and clean the data faster and more easy and with higher quality for further use with ML tools",
		"activities": [
			{
				"name": "SFTP_to_Blob_schema_parquet",
				"description": "copy data from the FTP server to BlobStorage",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "SftpReadSettings",
							"recursive": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "account_yy_ss",
									"type": "Int32"
								},
								"sink": {
									"name": "account_yy_ss",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "Province",
									"type": "String"
								},
								"sink": {
									"name": "province",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "Type",
									"type": "String"
								},
								"sink": {
									"name": "type",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "hosp_serv_id",
									"type": "String"
								},
								"sink": {
									"name": "hosp_serv_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "reimbt_cat_id",
									"type": "String"
								},
								"sink": {
									"name": "reimbt_cat_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "drug_code",
									"type": "String"
								},
								"sink": {
									"name": "drug_code",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "realization_date",
									"type": "String"
								},
								"sink": {
									"name": "realization_date",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "quantity",
									"type": "String"
								},
								"sink": {
									"name": "quantity",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "amount_reimb",
									"type": "Single"
								},
								"sink": {
									"name": "amount_reimb",
									"type": "Single"
								}
							},
							{
								"source": {
									"name": "amount_not_reimb",
									"type": "Single"
								},
								"sink": {
									"name": "amount_not_reimb",
									"type": "Single"
								}
							},
							{
								"source": {
									"name": "Year",
									"type": "Int32"
								},
								"sink": {
									"name": "year",
									"type": "Int32"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "DelimitedText_from_SFTP",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "to_blob_schema_to_parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}