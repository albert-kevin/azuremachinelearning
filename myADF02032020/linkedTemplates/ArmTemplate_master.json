{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "myADF02032020"
		},
		"AzureBatch1_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch1'"
		},
		"AzureFunction1_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AzureFunction1'"
		},
		"BlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage1'"
		},
		"dataforbetterhealth_SFTP_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dataforbetterhealth_SFTP'"
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "azurebatchsv532020"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://azurebatchsv532020.westeurope.batch.azure.com"
		},
		"AzureBatch1_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "kevinPool18"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://docs.google.com/uc?export=download&id=1RU1LCr3HlAW9VQz2DUDXTf4n96gHaQZ3"
		},
		"dataforbetterhealth_SFTP_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "94.143.190.243"
		},
		"dataforbetterhealth_SFTP_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hackathon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureFunction1_functionKey": {
						"value": "[parameters('AzureFunction1_functionKey')]"
					},
					"BlobStorage1_connectionString": {
						"value": "[parameters('BlobStorage1_connectionString')]"
					},
					"dataforbetterhealth_SFTP_password": {
						"value": "[parameters('dataforbetterhealth_SFTP_password')]"
					},
					"AzureBatch1_accessKey": {
						"value": "[parameters('AzureBatch1_accessKey')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"dataforbetterhealth_SFTP_properties_typeProperties_host": {
						"value": "[parameters('dataforbetterhealth_SFTP_properties_typeProperties_host')]"
					},
					"dataforbetterhealth_SFTP_properties_typeProperties_userName": {
						"value": "[parameters('dataforbetterhealth_SFTP_properties_typeProperties_userName')]"
					},
					"AzureBatch1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_accountName')]"
					},
					"AzureBatch1_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]"
					},
					"AzureBatch1_properties_typeProperties_poolName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_poolName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}